<odoo>
    <record id="view_vehicle_tree" model="my_module.vehicle">
        <tree string="Vehicles">
            <field name="name"/>
            <field name="capacity"/>
        </tree>
    </record>

    <record id="action_vehicle" model="ir.actions.act_window">
        <field name="name">Vehicles</field>
        <field name="res.model" strip="true,form" type="string">
            <text name="vehicle">Vehicle</text>
            <theme id="view_vehicle_theme">
                <trip strip="name"/>
            </theme>
        </field>
    </record>
</odo>
